docker build -t us-central1-docker.pkg.dev/agents-stg/cloud-run-source-deploy/cloud-service-recommender:latest .
docker push us-central1-docker.pkg.dev/prodops-digitization/cloud-service-recommender:latest

us-central1-docker.pkg.dev/agents-stg/cloud-run-source-deploy

export GOOGLE_CLOUD_PROJECT=agents-stg
export GOOGLE_CLOUD_LOCATION=us-central1
export GOOGLE_GENAI_USE_VERTEXAI=True

gcloud run deploy cloud-service-recommender \
--source . \
--region $GOOGLE_CLOUD_LOCATION \
--project $GOOGLE_CLOUD_PROJECT \
--allow-unauthenticated \
--set-env-vars="GOOGLE_CLOUD_PROJECT=$GOOGLE_CLOUD_PROJECT,GOOGLE_CLOUD_LOCATION=$GOOGLE_CLOUD_LOCATION,GOOGLE_GENAI_USE_VERTEXAI=$GOOGLE_GENAI_USE_VERTEXAI"
# Add any other necessary environment variables your agent might need


curl "http://metadata.google.internal/computeMetadata/v1/instance/service-accounts/default/identity?audience=https://cloud-service-recommender-428871167882.us-central1.run.app" \
     -H "Metadata-Flavor: Google"


curl --location 'https://ui-backend-856717759313.us-central1.run.app/patients_detail' \
